// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Curriculo {
  id             Int          @id @default(autoincrement())
  name           String       @unique
  cpf            Int          @unique
  datanasc       String     
  email          String       @unique
  telefone       Int
  escolaridade   String
  funcao         String
  competencias   String

  @@map("curriculo")
}